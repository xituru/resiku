    "node-sass": "^6.0.1",
    "nuxt-class-component": "^1.3.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "sass-loader": "^10",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.3",
    "vue-jest": "^3.0.4"
  }
